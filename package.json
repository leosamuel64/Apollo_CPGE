{
	"name": "hermes-ocaml",
	"displayName": "Hermes - oCaml Extension",
	"description": "Be the Quickest in oCaml!",
	"version": "0.2.9",
	"publisher": "nyakimov",
	"icon": "images/hermes.png",
	"repository": {
		"type": "git",
		"url": "https://gitlab.com/otumn/hermes-ocaml.git"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Snippets"
	],
	"bugs": {
		"email": "nikita110598@mail.ru"
	},
	"contributes": {
		"snippets": [
			{
				"language": "ocaml",
				"path": "./snippets/snippets_ocaml.json"
			},
			{
				"language": "menhir",
				"path": "./snippets/snippets_menhir.json"
			}
		],
		"keybindings": [
			{
				"key": "alt+N",
				"command": "editor.action.insertSnippet",
				"when": "editorTextFocus",
				"editorLangId": "ocaml",
				"args": {
					"name": "Function"
				}
			},
			{
				"key": "alt+C",
				"command": "editor.action.insertSnippet",
				"when": "editorTextFocus",
				"editorLangId": "ocaml",
				"args": {
					"name": "Contract"
				}
			},
			{
				"key": "alt+M alt+M",
				"command": "editor.action.insertSnippet",
				"when": "editorTextFocus",
				"editorLangId": "ocaml,menhir",
				"args": {
					"name": "Compare"
				}
			},
			{
				"key": "alt+M alt+L",
				"command": "editor.action.insertSnippet",
				"when": "editorTextFocus",
				"editorLangId": "ocaml,menhir",
				"args": {
					"name": "Compare a List"
				}
			},
			{
				"key": "alt+M alt+O",
				"command": "editor.action.insertSnippet",
				"when": "editorTextFocus",
				"editorLangId": "ocaml,menhir",
				"args": {
					"name": "Compare an Option"
				}
			},
			{
				"key": "alt+F alt+G",
				"command": "editor.action.insertSnippet",
				"when": "editorTextFocus",
				"editorLangId": "ocaml,menhir",
				"args": {
					"name": "Fold Left"
				}
			},
			{
				"key": "alt+F alt+D",
				"command": "editor.action.insertSnippet",
				"when": "editorTextFocus",
				"editorLangId": "ocaml,menhir",
				"args": {
					"name": "Fold Right"
				}
			},
			{
				"key": "alt+L alt+M",
				"command": "editor.action.insertSnippet",
				"when": "editorTextFocus",
				"editorLangId": "ocaml,menhir",
				"args": {
					"name": "Map"
				}
			},
			{
				"key": "alt+B",
				"command": "editor.action.insertSnippet",
				"when": "editorTextFocus",
				"editorLangId": "ocaml",
				"args": {
					"name": "Test Block"
				}
			},
			{
				"key": "alt+!",
				"command": "editor.action.insertSnippet",
				"when": "editorTextFocus",
				"editorLangId": "ocaml,menhir",
				"args": {
					"name": "I don't know"
				}
			}
		]
	},
	"__metadata": {
		"id": "8ccf9440-9fdf-405d-812c-ea4913e1454f",
		"publisherId": "d64aa883-5ff3-48eb-8133-5cc99bbf7516",
		"publisherDisplayName": "Nikita Yakimovich"
	}
}